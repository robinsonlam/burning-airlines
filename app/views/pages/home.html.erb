<div id="main"></div>

<script id="appViewTemplate" type="template/underscore">
	<div id="searchForm">Text Area View goes here</div>
</script>

<script id="searchViewTemplate" type="template/underscore">
	<input type="text" id="originSearch" placeholder="Origin"></input>
	<input type="text" id="destinationSearch" placeholder="Destination"></input>
	<button>Search Flights</button>
</script>

<script id="resultsTemplate" type="template/underscore">
	<table>
		<thead>
			<td>Flight Number</td>
			<td>Origin</td>
			<td>Destination</td>
			<td>Flight Date</td>
			<td>Flight Time</td>
		<thead>
		<tbody id="flights">
		</tbody>
	</table>
</script>

<script id="flightTemplate" type="template/underscore">
	<tr class="flight">
		<td class="flight-number"></td>
		<td class="flight-origin"></td>
		<td class="flight-destination"></td>
		<td class="flight-date"></td>
		<td class="flight-time"></td>
	</tr>
</script>
